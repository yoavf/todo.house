# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_anon_key

# AI Provider Configuration
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-1.5-flash
DEFAULT_AI_PROVIDER=gemini
ENABLE_AI_USAGE_TRACKING=true

# Image Processing Configuration
MAX_IMAGE_SIZE_MB=10
MAX_AI_IMAGE_SIZE_KB=500
IMAGE_COMPRESSION_QUALITY=85

# Logging Configuration
LOG_LEVEL=INFO                  # DEBUG, INFO, WARNING, ERROR
LOG_FORMAT=json                 # json or text
LOG_INCLUDE_TIMESTAMP=true      # Include timestamp in JSON logs
LOG_INCLUDE_HOSTNAME=false      # Include hostname in JSON logs
LOG_INCLUDE_PROCESS_INFO=false  # Include process/thread info in JSON logs